<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:scrollbarAlwaysDrawVerticalTrack="false"
    android:scrollbarStyle="insideOverlay"
    android:background="#f6f6f6">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/scrollView"
        android:layout_marginBottom="50dp">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="5dp">

        <RelativeLayout
            android:id="@+id/signal_layout"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <TextView
                android:text="SignalStrength"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="5dp"
                android:textStyle="bold"
                android:gravity="start"
                android:padding="2dp"
                android:layout_margin="1dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />
            <TextView
                android:layout_alignParentEnd="true"
                android:padding="2dp"
                android:textStyle="italic"
                android:id="@+id/signal_strength"
                android:layout_margin="10dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true" />
        </RelativeLayout>
        <RelativeLayout
            android:layout_below="@+id/signal_layout"
            android:id="@+id/bit_error_rate"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <TextView
                android:text="GsmBitErrorRate"
                android:layout_marginLeft="10dp"
                android:gravity="start"
                android:padding="2dp"
                android:textStyle="bold"
                android:layout_margin="10dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />
            <TextView
                android:layout_alignParentEnd="true"
                android:padding="2dp"
                android:id="@+id/bit_error"
                android:textStyle="italic"
                android:layout_margin="10dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true" />
        </RelativeLayout>
            <RelativeLayout
                android:layout_below="@+id/bit_error_rate"
                android:id="@+id/cdma_dbm_layout"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <TextView
                    android:text="CdmaDbm"
                    android:layout_marginLeft="10dp"
                    android:gravity="start"
                    android:padding="2dp"
                    android:textStyle="bold"
                    android:layout_margin="10dp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />
                <TextView
                    android:layout_alignParentEnd="true"
                    android:padding="2dp"
                    android:id="@+id/cdma_dbm"
                    android:layout_margin="10dp"
                    android:textStyle="italic"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true" />
            </RelativeLayout>
            <RelativeLayout
                android:layout_below="@+id/cdma_dbm_layout"
                android:id="@+id/cdma_ecio_layout"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <TextView
                    android:text="CdmaEcio"
                    android:layout_marginLeft="10dp"
                    android:gravity="start"
                    android:padding="2dp"
                    android:textStyle="bold"
                    android:layout_margin="10dp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />
                <TextView
                    android:layout_alignParentEnd="true"
                    android:padding="2dp"
                    android:id="@+id/cdma_ecio"
                    android:layout_margin="10dp"
                    android:textStyle="italic"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true" />
            </RelativeLayout>
            <RelativeLayout
                android:layout_below="@+id/cdma_ecio_layout"
                android:id="@+id/evdo_dbm_layout"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <TextView
                    android:text="EvdoDbm"
                    android:layout_marginLeft="10dp"
                    android:gravity="start"
                    android:padding="2dp"
                    android:textStyle="bold"
                    android:layout_margin="10dp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />
                <TextView
                    android:layout_alignParentEnd="true"
                    android:padding="2dp"
                    android:textStyle="italic"
                    android:id="@+id/evdo_dbm"
                    android:layout_margin="10dp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true" />
            </RelativeLayout>
            <RelativeLayout
                android:layout_below="@+id/evdo_dbm_layout"
                android:id="@+id/evdo_ecio_layout"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <TextView
                    android:text="EvdoEcio"
                    android:layout_marginLeft="10dp"
                    android:gravity="start"
                    android:padding="2dp"
                    android:textStyle="bold"
                    android:layout_margin="10dp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />
                <TextView
                    android:layout_alignParentEnd="true"
                    android:padding="2dp"
                    android:id="@+id/evdo_ecio"
                    android:textStyle="italic"
                    android:layout_margin="10dp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true" />
            </RelativeLayout>
            <RelativeLayout
                android:layout_below="@+id/evdo_ecio_layout"
                android:id="@+id/evdo_snr_layout"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <TextView
                    android:text="EvdoSnr"
                    android:layout_marginLeft="10dp"
                    android:gravity="start"
                    android:padding="2dp"
                    android:textStyle="bold"
                    android:layout_margin="10dp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />
                <TextView
                    android:layout_alignParentEnd="true"
                    android:padding="2dp"
                    android:id="@+id/evdo_snr"
                    android:layout_margin="10dp"
                    android:textStyle="italic"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true" />
            </RelativeLayout>
            <RelativeLayout
                android:layout_below="@+id/evdo_snr_layout"
                android:id="@+id/lte_signal_strength_layout"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <TextView
                    android:text="LteSignalStrength"
                    android:layout_marginLeft="10dp"
                    android:gravity="start"
                    android:padding="2dp"
                    android:textStyle="bold"
                    android:layout_margin="10dp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />
                <TextView
                    android:layout_alignParentEnd="true"
                    android:padding="2dp"
                    android:textStyle="italic"
                    android:id="@+id/lte_signal_strength"
                    android:layout_margin="10dp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true" />
            </RelativeLayout>
            <RelativeLayout
                android:layout_below="@+id/lte_signal_strength_layout"
                android:id="@+id/lte_rsrp_layout"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <TextView
                    android:text="LteRsrp"
                    android:layout_marginLeft="10dp"
                    android:gravity="start"
                    android:padding="2dp"
                    android:textStyle="bold"
                    android:layout_margin="10dp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />
                <TextView
                    android:layout_alignParentEnd="true"
                    android:padding="2dp"
                    android:id="@+id/lte_rsrp"
                    android:textStyle="italic"
                    android:layout_margin="10dp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true" />
            </RelativeLayout>
            <RelativeLayout
                android:layout_below="@+id/lte_rsrp_layout"
                android:id="@+id/lte_rsrq_layout"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <TextView
                    android:text="LteRsrq"
                    android:layout_marginLeft="10dp"
                    android:gravity="start"
                    android:padding="2dp"
                    android:textStyle="bold"
                    android:layout_margin="10dp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />
                <TextView
                    android:layout_alignParentEnd="true"
                    android:padding="2dp"
                    android:id="@+id/lte_rsrq"
                    android:textStyle="italic"
                    android:layout_margin="10dp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true" />
            </RelativeLayout>
            <RelativeLayout
                android:layout_below="@+id/lte_rsrq_layout"
                android:id="@+id/lte_rssnr_layout"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <TextView
                    android:text="LteRssnr"
                    android:layout_marginLeft="10dp"
                    android:gravity="start"
                    android:padding="2dp"
                    android:textStyle="bold"
                    android:layout_margin="10dp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />
                <TextView
                    android:layout_alignParentEnd="true"
                    android:padding="2dp"
                    android:id="@+id/lte_rssnr"
                    android:textStyle="italic"
                    android:layout_margin="10dp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true" />
            </RelativeLayout>
            <RelativeLayout
                android:layout_below="@+id/lte_rssnr_layout"
                android:id="@+id/lte_cqi_layout"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <TextView
                    android:text="LteCqi"
                    android:layout_marginLeft="10dp"
                    android:gravity="start"
                    android:padding="2dp"
                    android:textStyle="bold"
                    android:layout_margin="10dp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />
                <TextView
                    android:layout_alignParentEnd="true"
                    android:padding="2dp"
                    android:id="@+id/lte_cqi"
                    android:layout_margin="10dp"
                    android:textStyle="italic"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true" />
            </RelativeLayout>
            <RelativeLayout
                android:layout_below="@+id/lte_cqi_layout"
                android:id="@+id/gsm_layout"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <TextView
                    android:text="Gsm|Lte|Cdma"
                    android:layout_marginLeft="10dp"
                    android:gravity="start"
                    android:padding="2dp"
                    android:textStyle="bold"
                    android:layout_margin="10dp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />
                <TextView
                    android:layout_alignParentEnd="true"
                    android:padding="2dp"
                    android:id="@+id/gsm"
                    android:textStyle="italic"
                    android:layout_margin="10dp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true" />
            </RelativeLayout>

        </RelativeLayout>
    </ScrollView>

    <Button
        android:id="@+id/upload"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:text="Upload"
        android:background="#00A1D8"
        android:textColor="@android:color/white"
        android:layout_gravity="center_horizontal"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true" />

</RelativeLayout>
